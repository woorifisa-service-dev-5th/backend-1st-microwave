package dev.syntax.thread;

public class Print {
 public static void main(String[] args) {
	        System.out.println("문이 닫혔습니다.");
	        System.out.println("┌────────────────────────────────────────────────────────┐");
	        System.out.println("│                                                        │");
	        System.out.println("│                                                        │");
	        System.out.println("│  ┌────────────────────────┐  ┌───────────────────┐   │");
	        System.out.println("│  │  .---.                 │  │ 🎛️  [ o ]        │   │");
	        System.out.println("│  │ (     )                │  │                   │   │");
	        System.out.println("│  │  '---'                 │  │ 1  2  3  4        │   │");
	        System.out.println("│  │         지이이이잉...  │  │ 5  6  7  8        │   │");
	        System.out.println("│  │   🔥                   │  │ 9  0  ▶️  ⏹️        │   │");
	        System.out.println("│  └────────────────────────┘  └───────────────────┘   │");
	        System.out.println("│                                                        │");
	        System.out.println("└────────────────────────────────────────────────────────┘");
	    
 }
}
