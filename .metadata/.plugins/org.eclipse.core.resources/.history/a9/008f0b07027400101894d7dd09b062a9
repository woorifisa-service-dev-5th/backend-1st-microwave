package dev.syntax.thread;

public class Print {
	    public static void main(String[] args) {
	        System.out.println("문이 닫혔습니다. 음식을 데우는 중입니다... 🔥");
	        System.out.println("┌──────────────────────────────────┐");
	        System.out.println("│  ┌───────────────┐ ┌────────────┐  ");
	        System.out.println("│  │  .---.        │ │ 🎛️         │  ");
	        System.out.println("│  │ (     )       │ │ 1  2  3    │  ");
	        System.out.println("│  │  '---'        │ │ 4  5  6    │  ");
	        System.out.println("│  │               │ │ 7  8  9    │  ");
	        System.out.println("│  │   🔥          │ │ ▶️ 0  ⏹️   │  ");
	        System.out.println("│  └───────────────┘ └────────────┘  ");
	        System.out.println("└──────────────────────────────────┘");
	    }
	
}
